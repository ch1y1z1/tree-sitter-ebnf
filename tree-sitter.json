  "grammars": [
    {
      "name": "ebnf",
      "camelcase": "Ebnf",
      "title": "Ebnf",
      "scope": "source.ebnf",
      "file-types": [
        "ebnf"
      ],
      "injection-regex": "^ebnf$",
      "class-name": "TreeSitterEbnf"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Ebnf grammar for tree-sitter",
    "authors": [
      {
        "name": "chiyizi",
        "email": "ch1y1z1kun@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ebnf"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}